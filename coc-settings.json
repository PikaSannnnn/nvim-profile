{
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": ["javascript", "javascriptreact", "typescript", "typescriptreact", "python", "go", "c", "cpp"],
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "python",
    "go",
    "c",
    "cpp"
  ],
  "tsserver.formatOnType": true,
  "coc.preferences.formatOnType": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "suggest.noselect": true,
  "suggest.enablePreselect": false,
  "coc.preferences.triggerCompletionWait": 50,
  "coc.preferences.snippetsSupport": true
}


